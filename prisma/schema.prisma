generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model About {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
  detail     String?
  image      String?
  subtitle   String?
  title      String?
}

model Contact {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title      String?
  address    String?
  tel        String?
  email      String?
  facebook   String?
  line       String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
  linkmap    String?
}

model Homee {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title1     String?
  subtitle1  String?
  detail1    String?
  imageh1    String?
  title2     String?
  subtitle2  String?
  detail2    String?
  imageh2    String?
  title3     String?
  subtitle3  String?
  detail3    String?
  imageh3    String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
}

model Products {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
  detail     String?
  image      String?
  subtitle   String?
  title      String?
}

model Headproduct {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  header     String?
  subheader  String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
}

model Homedetail {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  title      String?
  subtitle   String?
  detail     String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
}

model Hometop {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  image      String?
  name       String?
  subname    String?
  link       String?
  createdAt  DateTime? @default(now())
  updatedAt  DateTime?
  deleted_at DateTime?
}

model Gallery {
  id         String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  image      String?
  createdAt  DateTime? @default(now())
  deleted_at DateTime?
}
